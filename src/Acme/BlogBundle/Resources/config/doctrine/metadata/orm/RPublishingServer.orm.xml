<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="RPublishingServer" table="r_publishing_server">
    <id name="idServer" type="integer" column="id_server">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="cname" type="string" column="cname" length="45" nullable="false"/>
    <field name="isMaster" type="boolean" column="is_master" nullable="false"/>
    <field name="isSupermonitor" type="boolean" column="is_supermonitor" nullable="false"/>
    <field name="apiLocation" type="string" column="api_location" length="45" nullable="false"/>
  </entity>
</doctrine-mapping>
